{
  "env": {
    "development": {
      "plugins": [
        [
          "babel-plugin-styled-components",
          {
            "ssr": true,
            "displayName": true,
            "preprocess": false,
            "topLevelImportPaths": [
              "@xstyled/styled-components",
              "@xstyled/styled-components/no-tags",
              "@xstyled/styled-components/native",
              "@xstyled/styled-components/primitives"
            ]
          }
        ]
      ],
      "presets": ["next/babel"]
    },
    "production": {
      "plugins": [
        [
          "babel-plugin-styled-components",
          {
            "ssr": true,
            "displayName": true,
            "preprocess": false,
            "topLevelImportPaths": [
              "@xstyled/styled-components",
              "@xstyled/styled-components/no-tags",
              "@xstyled/styled-components/native",
              "@xstyled/styled-components/primitives"
            ]
          }
        ]
      ],
      "presets": ["next/babel"]
    },
    "test": {
      "presets": ["next/babel"]
    }
  },
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false,
        "topLevelImportPaths": [
          "@xstyled/styled-components",
          "@xstyled/styled-components/no-tags",
          "@xstyled/styled-components/native",
          "@xstyled/styled-components/primitives"
        ]
      }
    ]
  ]
}
